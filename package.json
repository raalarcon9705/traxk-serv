{
  "name": "track-serv",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "db:push": "dotenv -e .env -- npx supabase db push",
    "db:reset": "dotenv -e .env -- npx supabase db reset",
    "db:types": "dotenv -e .env -- sh -c 'npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > app/supabase/types.ts'",
    "db:types-local": "dotenv -e .env -- npx supabase gen types typescript --local > app/supabase/types.ts",
    "functions:deploy": "dotenv -e .env -- npx supabase functions deploy",
    "functions:serve": "dotenv -e .env -- npx supabase functions serve",
    "functions:deploy-all": "dotenv -e .env -- npx supabase functions deploy --no-verify-jwt",
    "supabase:link": "dotenv -e .env -- sh -c 'npx supabase link --project-ref $SUPABASE_PROJECT_ID'",
    "supabase:status": "dotenv -e .env -- npx supabase status",
    "supabase:projects": "dotenv -e .env -- npx supabase projects list",
    "supabase:login": "npx supabase login",
    "supabase:logout": "npx supabase logout"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.2",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "isbot": "^5.1.31",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-is": "^19.1.1",
    "react-router": "^7.9.2",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "dotenv-cli": "^10.0.0",
    "jsdom": "^27.0.0",
    "supabase": "^2.45.5",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  }
}
